<section id="contenedorBasico">
<h1>Lista de Tipos de Servicios</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @service_types.each do |service_type| %>
  <tr>
    <td><%= service_type.name %></td>
    <td><%= link_to 'Mostrar', service_type %></td>
    <td><%= link_to 'Editar', edit_service_type_path(service_type) %></td>
    <td><%= link_to 'Eliminar', service_type, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Tipo de Servicio', new_service_type_path %>
</section>