<h1>Listing publicities</h1>

<table>
  <tr>
    <th>Titulo</th>
    <th>Contenido</th>
    <th>Fechainicio</th>
    <th>Fechafin</th>
    <th>Tarifa</th>
    <th>Nroclick</th>
    <th>Ubications</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @publicities.each do |publicity| %>
  <tr>
    <td><%= publicity.titulo %></td>
    <td><%= publicity.contenido %></td>
    <td><%= publicity.fechaInicio %></td>
    <td><%= publicity.fechaFin %></td>
    <td><%= publicity.tarifa %></td>
    <td><%= publicity.nroclick %></td>
    <td><%= publicity.ubications %></td>
    <td><%= link_to 'Show', publicity %></td>
    <td><%= link_to 'Edit', edit_publicity_path(publicity) %></td>
    <td><%= link_to 'Destroy', publicity, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

