<%= simple_form_for(@tarifa) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">

    <%= f.association :cancha %>

    <%= f.input :promocion %>

    <%= f.association :dia_semana %>

    <%= f.input :start_time %>

    <%= f.input :end_time %>

    <%= f.input :price %>

  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
